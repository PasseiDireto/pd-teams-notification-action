{
    "name": "pd-teams-notification-action",
    "description": "GitHub Action to send MS Teams Notification",
    "version": "0.0.1",
    "private": true,
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "main": "dist/main.js",
    "scripts": {
        "prepare": "husky install",
        "build": "rm -rf ./dist && npx tsc",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "lint-staged": "lint-staged",
        "commit": "cz",
        "update-all": "ncu -u",
        "pack": "rm -rf ./lib && ncc build -m -o lib",
        "all": "npm run build && npm run pack",
        "publish": "NODE_OPTIONS=--openssl-legacy-provider npm run all"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "lint-staged": {
        "*.js": "eslint --fix"
    },
    "dependencies": {
        "@actions/core": "^1.10.0",
        "@octokit/graphql": "^5.0.5",
        "@octokit/rest": "^19.0.8",
        "axios": "^1.4.0",
        "moment-timezone": "^0.5.43",
        "octokit": "^2.0.15"
    },
    "devDependencies": {
        "@types/moment-timezone": "^0.5.30",
        "@types/node": "^20.2.1",
        "@typescript-eslint/eslint-plugin": "^5.59.6",
        "@typescript-eslint/parser": "^5.59.6",
        "@zeit/ncc": "^0.20.5",
        "commitizen": "^4.3.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-github": "^4.7.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "js-yaml": "^4.1.0",
        "lint-staged": "^13.2.2",
        "npm-check-updates": "^16.10.12",
        "prettier": "^2.8.8",
        "typescript": "^5.0.4"
    }
}